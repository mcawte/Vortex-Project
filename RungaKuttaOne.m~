function [ ynext ] = RungaKuttaOne(PSI,k,g,DeltaT)
% First attempt at Runga Kutta Method


k1 = RHSdPSI(PSI,k,g,DeltaT);

k2 = RHSdPSI(PSI + DeltaT/2 .* k1,k,g,DeltaT); 

k3 = RHSdPSI(PSI + DeltaT/2 .* k2,k,g,DeltaT); 

k4 = RHSdPSI(PSI + DeltaT/2 .* k2,k,g,DeltaT); 


ynext = yn + DeltaT/6 * (k1 + 2*k2 + 2*k3 + k4);



end

